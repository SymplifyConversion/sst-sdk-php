  "name": "symplify-conversion/sst-sdk-php",
  "description": "SDK for Symplify Conversion Server-Side Testing",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Symplify Technologies",
      "email": "conversion@symplify.com",
      "homepage": "https://symplify.com/conversion/"
    },
    {
      "name": "Fabian BergstrÃ¶m",
      "role": "developer"
    },
